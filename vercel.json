{
  "rewrites": [
    {
      "source": "/(?!_next|api|public|.*\\..*$)(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
